ush@kush-Lenovo-Z580:~$ vim index.js
kush@kush-Lenovo-Z580:~$ cat dockerfile
cat: dockerfile: No such file or directory
kush@kush-Lenovo-Z580:~$ vi dockerfile

kush@kush-Lenovo-Z580:~$ vim dockerfile_alpine 
kush@kush-Lenovo-Z580:~$ cat dockerfile_alpine 
FROM alpine
MAINTAINER kavit
RUN apk update  
RUN apk add nodejs 
COPY index.js /app/
WORKDIR "/app"
CMD node index.js
